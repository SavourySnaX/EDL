# Test harness for 65816

INSTANCE "chips/WD65816.edl" AS CPU;
INSTANCE "chips/ROM.EDL" AS ROM;

INSTANCE "chips/dummy.edl" AS CONSUME;

CONNECT TEST , RESET
{
	BUS_TAP("PHI2", 0) CPU PIN_PHI2, TEST;
	BUS_TAP("RESET",0) CPU PIN_RESB, RESET;
	BUS_TAP("A",16) CPU PIN_A, ROM A;	
	BUS_TAP("D",8) CPU PIN_D, ROM D;

	BUS_TAP("MX",0) CPU PIN_MX, CONSUME DUMMY0;
	BUS_TAP("E",0) CPU PIN_E, CONSUME DUMMY0;
	BUS_TAP("RWB",0) CPU PIN_RWB, CONSUME DUMMY0;
	BUS_TAP("VPA",0) CPU PIN_VPA, CONSUME DUMMY0;
	BUS_TAP("VDA",0) CPU PIN_VDA, CONSUME DUMMY0;
	BUS_TAP("MLB",0) CPU PIN_MLB, CONSUME DUMMY0;
}
